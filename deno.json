{
  "tasks": {
    "build": "deno run -A main.ts",
    "dev": "nodemon -e tsx,json,ts,css -i dist --exec deno run -A main.ts"
  },
  "fmt": {
    "options": {
      "lineWidth": 90
    }
  },
  "importMap": "import_map.json",
  "exclude": [
    "./dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "npm:preact",
    "lib": [
      "deno.ns",
      "dom"
    ]
  }
}